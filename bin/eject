if grep -qs "${1} " /proc/mounts; then
  sudo umount -v ${1}?*
fi

if ! grep -qs "${1} " /proc/mounts; then
  sudo /usr/bin/eject ${1}
  sudo sg_sync ${1}
  sudo sg_start 0 ${1}
  sudo sg_start --eject ${1}
else
  echo "eject failed!"
fi

